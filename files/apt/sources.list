#############################################################################
#
# UBUNTU 22.04 (JAMMY JELLYFISH)
# /etc/apt/sources.list
#
#############################################################################

#################################################
#
# MAIN & RESTRICTED
# Officially Supported, Open-Source Software
# & Officially Supported, Closed-Source Software
#
#################################################

deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy          main restricted
deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy-updates  main restricted
deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy-security main restricted

#################################################
#
# UNIVERSE
# Community-Maintained, Open-Source Software
#
#################################################

deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy          universe
deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy-updates  universe
deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy-security universe

#################################################
#
# MULTIVERSE
# Unsupported, Closed-Source and Patent-Encumbered Software
#
#################################################

deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy          multiverse
deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy-updates  multiverse
deb [arch=amd64] http://de.archive.ubuntu.com/ubuntu/ jammy-security multiverse
