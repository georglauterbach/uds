# VS Code
set_from_resource $i3-wm.binding.code i3-wm.binding.code c
bindsym $mod+$i3-wm.binding.code exec code

# Move workspace to another monitor (to the left)
set_from_resource $i3-wm.binding.move_ws i3-wm.binding.move_ws m
bindsym $mod+$i3-wm.binding.move_ws move workspace to output left

set_from_resource $i3-wm.binding.toggle_bar i3-wm.binding.toggle_bar i
bindsym $mod+$i3-wm.binding.toggle_bar exec polybar-msg cmd toggle
